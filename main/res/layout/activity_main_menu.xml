<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="errormjt.mason980.fretter_android.fretter.MainMenuActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main_menu"
        app:layout_constraintTop_toTopOf="@+id/activity_main_menu"
        app:layout_constraintRight_toRightOf="@+id/activity_main_menu"
        app:layout_constraintBottom_toBottomOf="@+id/activity_main_menu"
        app:layout_constraintHorizontal_bias="0.37"
        app:layout_constraintVertical_bias="0.46" />

    <Button
        android:text="Begin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="143dp"
        android:id="@+id/button"
        android:onClick="openScreen" />

</android.support.constraint.ConstraintLayout>
